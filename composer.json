{
    "name": "markup/addressing-bundle",
    "description": "Integration of markup/addressing with Symfony2.",
    "keywords": ["addresses", "postal", "addressing", "formatting"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Douglas Greenshields",
            "email": "douglas@usemarkup.com"
        },
        {
            "name": "Markup",
            "homepage": "http://www.usemarkup.com/"
        }
    ],
    "require": {
        "markup/addressing": "1.0.x-dev",
        "symfony/framework-bundle": "~2.3"
    },
    "require-dev": {
        "symfony/validator": "~2.3",
        "phpunit/phpunit": "3.7.*",
        "mockery/mockery": "~0.8.0"
    },
    "suggests": {
        "symfony/validator": "~2.3"
    },
    "autoload": {
        "psr-0": { "Markup\\AddressingBundle": "" }
    },
    "target-dir": "Markup/AddressingBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
